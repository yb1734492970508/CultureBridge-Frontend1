# 生产环境配置
VITE_APP_NAME=CultureBridge
VITE_APP_VERSION=3.0.0
VITE_APP_ENVIRONMENT=production

# API配置
VITE_API_BASE_URL=https://api.culturebridge.app
VITE_API_TIMEOUT=15000
VITE_API_RETRY_ATTEMPTS=3

# WebSocket配置
VITE_SOCKET_URL=https://api.culturebridge.app
VITE_SOCKET_TIMEOUT=10000
VITE_SOCKET_RECONNECT_ATTEMPTS=10

# 区块链配置
VITE_BLOCKCHAIN_NETWORK=mainnet
VITE_CBT_CONTRACT_ADDRESS=0x742d35Cc6634C0532925a3b8D4C2F8b4C4b4b4b4
VITE_BSC_RPC_URL=https://bsc-dataseed1.binance.org/
VITE_BSC_CHAIN_ID=56

# 语音翻译配置
VITE_SPEECH_API_ENABLED=true
VITE_SUPPORTED_LANGUAGES=en,zh,ja,ko,es,fr,de,it,pt,ru,ar,hi,th,vi

# 功能开关
VITE_FEATURE_VOICE_CHAT=true
VITE_FEATURE_BLOCKCHAIN=true
VITE_FEATURE_REAL_TIME_TRANSLATION=true
VITE_FEATURE_CULTURAL_CONTENT=true
VITE_FEATURE_GAMIFICATION=true

# 性能配置
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# 安全配置
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true
VITE_SESSION_TIMEOUT=1800000

# UI配置
VITE_DEFAULT_THEME=light
VITE_ENABLE_ANIMATIONS=true
VITE_ANIMATION_DURATION=200

